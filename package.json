{
  "name": "x402-starknet",
  "version": "0.1.2",
  "description": "Starknet implementation of the x402 payment protocol",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "bun run build:ts",
    "build:ts": "tsc -p tsconfig.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "dev": "bun run --watch src/index.ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "x402",
    "starknet",
    "payments",
    "micropayments",
    "http",
    "blockchain",
    "cairo"
  ],
  "author": "Nethermind",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/NethermindEth/x402-starknet"
  },
  "bugs": {
    "url": "https://github.com/NethermindEth/x402-starknet/issues"
  },
  "homepage": "https://github.com/NethermindEth/x402-starknet/blob/main/README.md",
  "dependencies": {
    "@scure/base": "^1.1.10",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/coverage-v8": "^3.0.0",
    "eslint": "^9.0.0",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-unicorn": "^62.0.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globals": "^16.5.0",
    "prettier": "^3.4.0",
    "starknet": "^8.7.0",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.46.3",
    "vitest": "^3.0.0"
  },
  "peerDependencies": {
    "starknet": "^8.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
